# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Builder.io API Key
VITE_PUBLIC_BUILDER_KEY=your_builder_key_here

# Sentry configuration (optional)
VITE_SENTRY_DSN=your_sentry_dsn_here

# Development settings
VITE_BYPASS_PLAN_GUARD=true
VITE_STORE_ENABLED=false
VITE_DEV_MODE=true

# Admin configuration
VITE_ADMIN_EMAIL=your_admin@example.com

# API URLs
VITE_API_URL=http://localhost:3000/api

# Stripe configuration
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key
VITE_PAYMENT_URL=https://your-payment-url.com

# Google Cloud Storage (optional)
VITE_GCS_BUCKET_NAME=your-bucket-name
VITE_GCS_PROJECT_ID=your-project-id
VITE_GCS_API_KEY=your-gcs-api-key

# Supabase configuration (REQUIRED)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_SUPABASE_DEMO_MODE=false

# Optional features
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PERFORMANCE_MONITORING=false
