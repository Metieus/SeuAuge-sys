# API endpoint used by src/services/api.ts
VITE_API_URL=http://localhost:3000/api

# Firebase configuration - Replace with your actual Firebase project config
VITE_FIREBASE_API_KEY=demo-api-key-placeholder
VITE_FIREBASE_AUTH_DOMAIN=demo-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=demo-project
VITE_FIREBASE_STORAGE_BUCKET=demo-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdefghijk

# Sentry configuration
VITE_SENTRY_DSN=

# Bypass plan restriction during development
VITE_BYPASS_PLAN_GUARD=true

# Enable store and products features
VITE_STORE_ENABLED=false

# Development mode bypass for Firebase authentication
VITE_DEV_MODE=true

# Stripe configuration
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key_here
VITE_STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here
